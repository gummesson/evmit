{
  "name": "evmit",
  "version": "0.1.0",
  "description": "A tiny, modern, bare bones event emitter.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js",
    "browser": "browserify test/index.js | tape-run",
    "clean": "rm -rf dist && mkdir dist",
    "lint": "jshint index.js test/index.js",
    "bundle": "browserify -s Evmit index.js -o dist/evmit.js",
    "minify": "uglifyjs dist/evmit.js -m -o dist/evmit.min.js",
    "dist": "npm run clean && npm run bundle && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gummesson/evmit.git"
  },
  "keywords": [
    "event",
    "emitter",
    "browser"
  ],
  "author": "Ellen Gummesson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gummesson/evmit/issues"
  },
  "homepage": "https://github.com/gummesson/evmit",
  "devDependencies": {
    "browserify": "^3.44.2",
    "jshint": "^2.5.0",
    "uglify-js": "^2.4.13",
    "tape": "^2.12.3",
    "tape-run": "^0.1.1"
  }
}
